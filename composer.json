{
    "name": "mmoreram/paymill-bundle",
    "type": "symfony-bundle",
    "description": "Paymill Bundle, part of Mmoreram Payment Suite",
    "keywords": ["paymill", "payment", "ecommerce"],
    "license": "MIT",
    "authors": [
        {
            "name": "Marc Morera",
            "email": "yuhu@mmoreram.com"
        }
    ],
    "require": {
        "php": ">=5.3.3",
        "symfony/event-dispatcher": ">=2.2.3",
        "symfony/framework-bundle": ">=2.2.3",
        "symfony/form": ">=2.2.3",
        "symfony/config": ">=2.2.3",
        "symfony/http-kernel": ">=2.2.3",
        "symfony/dependency-injection": ">=2.2.3",
        "twig/twig": "~1.11",
        "paymill/paymill": "2.1.0",
        "mmoreram/payment-core-bundle": "~1"
    },
    "require-dev": {
        "phpunit/phpunit": "3.7.*"
    },
    "target-dir": "Mmoreram/PaymillBundle",
    "autoload": {
        "psr-0": { "Mmoreram\\PaymillBundle": "" }
    },
    "minimum-stability": "stable", 
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
